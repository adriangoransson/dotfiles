[color]
    ui = auto
[user]
	name = Adrian GÃ¶ransson
    # Set email per project
[core]
    pager =  diff-highlight | less
[alias]
    ci = commit -v
    new = !sh -c 'git log $1@{1}..$1@{0} "$@"'
    dfst = diff --stat -r
    ch = checkout
    pl = pull
    ph = push
    f = fetch --tags
    fa = fetch --all --tags
    r = reset
    rh = reset --hard
    st = status
    df = diff
    l = log --date=local
    del = branch -d
    fdel = branch -D

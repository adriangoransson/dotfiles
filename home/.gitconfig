[core]
[alias]
    ci = commit -v
    ch = checkout
    pl = pull
    ph = push
    f = fetch --tags
    fa = fetch --all --tags
    r = reset
    rh = reset --hard
    st = status
    df = diff
    dfst = diff --stat -r
    dff = !git diff | diff-so-fancy | less --tabs=4 -RFX
    l = log --date=local
